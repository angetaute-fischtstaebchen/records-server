{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "plugin:import/recommended",
    "plugin:prettier/recommended",
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier", "mongoose"],
  "rules": {
    "import/prefer-default-export": "off",
    "import/no-default-export": "error"
  }
}
